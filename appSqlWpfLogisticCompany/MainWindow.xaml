<Window x:Class="appSqlWpfLogisticCompany.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:appSqlWpfLogisticCompany"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000">

    <Window.Background>
        <ImageBrush ImageSource="Resources/truck.jpg" Stretch="UniformToFill"/>
    </Window.Background>
    
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Rectangle Fill="#606EB2" Grid.RowSpan="2" Opacity="0.8"></Rectangle>

        <Frame Grid.RowSpan="2" Name="MainFraim" Grid.Row="1" NavigationUIVisibility="Hidden">
            
        </Frame>
    </Grid>
</Window>
